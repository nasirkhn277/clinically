<%- include('head.ejs') %>
<body>
	<%- include('includes/header.ejs') %>
<div class="wrapper">
	
	<div class="col-wrapper">
		<%- include('includes/sidebarmenu.ejs') %>
		<div class="full-column db-cont-wrap right">
		
			<div class="column-head">
				<div class="col-pagename left">My Clinics</div>
				<div class="master-creation right"><a href="clinics/addclinic"><span class="crticn"><img src="img/plus.svg" alt="" width="16px"></span><span class="crtxt">Add New Clinic</span><div class="clr"></div></a></div>
